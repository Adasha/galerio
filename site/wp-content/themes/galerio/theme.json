                {
                    "color": "#303030",
                    "name": "Contrast",
                    "slug": "contrast"
                },
                {
                    "color": "#e92323",
                    "name": "Primary",
                    "slug": "primary"
                },
                {
                    "color": "#bd1818",
                    "name": "Secondary",
                    "slug": "secondary"
                },
                {
                    "color": "#767676",
                    "name": "Lower contrast",
                    "slug": "custom-lower-contrast"
                },
                {
                    "color": "#e7e7e7",
                    "name": "Highlight",
                    "slug": "custom-highlight"
                },
                {
